    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "happy-dom": "^14.12.3",
    "postcss": "^8.4.39",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^12.1.5",
    "prettier": "^3.3.3",
